EVENT_API_JWT_PRIVATE_KEY=<%= @barong_private_key %>
EVENT_API_JWT_ALGORITHM=RS256
EVENT_API_RABBITMQ_HOST=rabbitmq
EVENT_API_RABBITMQ_PORT=5672
EVENT_API_RABBITMQ_USERNAME=guest
EVENT_API_RABBITMQ_PASSWORD=guest

VAULT_TOKEN=<%= @config['vault']['token'] %>
VAULT_ENDPOINT=http://vault:8200

FRONTEND_EXCHANGE_OPENDAX_DOMAIN=<%= @config['app']['subdomain'] %>.<%= @config['app']['domain'] %>

<%- if @config['ssl']['enabled'] -%>
FRONTEND_EXCHANGE_OPENDAX_URL=https://<%= @config['app']['subdomain'] %>.<%= @config['app']['domain'] %>
<%- else -%>
FRONTEND_EXCHANGE_OPENDAX_URL=http://<%= @config['app']['subdomain'] %>.<%= @config['app']['domain'] %>
<%- end -%>
